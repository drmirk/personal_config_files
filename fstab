# /etc/fstab: static file system information.
#
# Use 'blkid' to print the universally unique identifier for a
# device; this may be used with UUID= as a more robust way to name devices
# that works even if disks are added and removed. See fstab(5).
#
# <file system> <mount point>   <type>  <options>       <dump>  <pass>
# / was on /dev/sda2 during installation
UUID=3bdc86d4-202e-4b05-8aaf-5315f14b37ed /               ext4    noatime,errors=remount-ro 0       1
# /boot/efi was on /dev/sda1 during installation
UUID=0E72-6EE9  /boot/efi       vfat    umask=0077      0       1
/swapfile                                 none            swap    sw              0       0
/dev/sr0        /media/cdrom0   udf,iso9660 user,noauto     0       0


# all_files
UUID=8a02fa6f-72ba-42f8-bbf5-6e7c6adaf3af	/mnt/all_files	ext4	rw,nosuid,nodev,auto,user,exec,async,noatime	0	0

# TMPFS
tmpfs	/tmp	tmpfs	rw,nodev,nosuid,noatime,size=4G	0	0
tmpfs	/var/spool	tmpfs	rw,noexec,nodev,nosuid,noatime,size=1G	0	0 
tmpfs	/var/log	tmpfs	rw,noexec,nodev,nosuid,noatime,size=1G	0	0
