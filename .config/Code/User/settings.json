{
  "breadcrumbs.enabled": false,
  "C_Cpp.clang_format_path": "/usr/bin/clang-format",
  "C_Cpp.clang_format_style": "{ BasedOnStyle: LLVM, IndentWidth: 2, TabWidth: 2, UseTab: Always, AllowShortFunctionsOnASingleLine: false }",
  "code-runner.clearPreviousOutput": true,
  "code-runner.enableAppInsights": false,
  "code-runner.executorMap": {
    "c": "cd $dir && gcc -ggdb3 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "dart": "cd $workspaceRoot\/bin && dart main.dart",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "python": "python -u",
    "shellscript": "bash"
  },
  "code-runner.ignoreSelection": true,
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,
  "dart.allowAnalytics": false,
  "dart.flutterCreateAndroidLanguage": "kotlin",
  "dart.flutterCreateIOSLanguage": "swift",
  "debug.allowBreakpointsEverywhere": true,
  "debug.inlineValues": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.cursorStyle": "line",
